{
  "compilerOptions": {
    "esModuleInterop": true,
    "module":"commonjs",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/routes/items.ts", "pages/api/mongo.mts", "pages/api/expressapi.mts"],
  "exclude": ["node_modules"]
}
